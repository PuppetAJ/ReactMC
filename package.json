{
  "name": "reactmc",
  "version": "1.0.0",
  "description": "Minecraft remade using React",
  "main": "main.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PuppetAJ/ReactMC.git"
  },
  "author": "Adrian Jimenez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PuppetAJ/ReactMC/issues"
  },
  "homepage": "https://github.com/PuppetAJ/ReactMC#readme",
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "dependencies": {
    "@react-three/drei": "^9.46.3"
  }
}
